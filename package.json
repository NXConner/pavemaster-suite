  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:mobile": "vite build --mode production && npx cap sync",
    "mobile:sync": "npx cap sync",
    "mobile:build": "npm run build && npx cap sync",
    "mobile:ios": "npm run mobile:build && npx cap open ios",
    "mobile:android": "npm run mobile:build && npx cap open android",
    "mobile:ios:dev": "npx cap run ios",
    "mobile:android:dev": "npx cap run android",
    "mobile:live-reload": "npx cap run ios --livereload --external",
    "mobile:live-reload:android": "npx cap run android --livereload --external",
    "analyze-bundle": "npx vite-bundle-analyzer dist/stats.json",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:a11y": "axe-playwright",
    "test:performance": "echo 'Performance testing disabled - lighthouse-ci removed for security'",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "analyze": "vite build --mode=analyze",
    "build:performance": "vite build && npm run analyze",
    "performance:audit": "node scripts/performance/lighthouse-audit.js",
    "performance:test": "npm run performance:audit http://localhost:8080",
    "cache:clear": "rm -rf node_modules/.vite && rm -rf dist",
    "build:production": "npm run cache:clear && vite build --mode=production",
    "serve:production": "vite preview --port 8080"
  },
