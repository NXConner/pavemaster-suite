# Environment Configuration
NODE_ENV=development
PORT=8080

# API Configuration
VITE_API_BASE_URL=http://localhost:3000/api
VITE_API_TIMEOUT=10000

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Authentication
VITE_AUTH_REDIRECT_URL=http://localhost:8080/auth/callback

# Maps and GIS
VITE_MAPBOX_TOKEN=your_mapbox_token
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_key

# Analytics
VITE_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
VITE_SENTRY_DSN=your_sentry_dsn

# Feature Flags
VITE_ENABLE_DEVELOPMENT_TOOLS=true
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false

# Security
VITE_ENABLE_CSP=true
VITE_ENABLE_HTTPS_ONLY=false

# Performance
VITE_ENABLE_PWA=true
VITE_ENABLE_SERVICE_WORKER=true

# Blockchain (if applicable)
VITE_BLOCKCHAIN_NETWORK=testnet
VITE_BLOCKCHAIN_API_KEY=your_blockchain_api_key

# IoT Configuration
VITE_IOT_MQTT_BROKER=wss://your-mqtt-broker.com
VITE_IOT_DEVICE_API_KEY=your_iot_api_key