# PaveMaster Suite Environment Configuration
# Copy this file to .env and fill in your actual values

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# Weather API
VITE_OPENWEATHER_API_KEY=your-openweather-api-key
OPENWEATHER_API_KEY=your-openweather-api-key

# Mapping Services
VITE_GOOGLE_MAPS_API_KEY=your-google-maps-api-key
VITE_MAPBOX_ACCESS_TOKEN=your-mapbox-access-token

# Payment Processing
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key
VITE_PAYPAL_CLIENT_ID=your-paypal-client-id

# AI/ML Services
VITE_TENSORFLOW_MODEL_URL=https://your-cdn.com/models/
VITE_COMPUTER_VISION_API_KEY=your-cv-api-key

# IoT & Device Integration
VITE_IOT_WEBSOCKET_URL=wss://your-iot-endpoint.com
IOT_DEVICE_API_KEY=your-iot-device-api-key

# Webhooks & Integrations
VITE_ZAPIER_WEBHOOK_URL=https://hooks.zapier.com/hooks/catch/your-webhook
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/your-slack-webhook

# Security & Monitoring
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
JWT_SECRET=your-jwt-secret-key
ENCRYPTION_KEY=your-encryption-key

# File Storage
VITE_AWS_S3_BUCKET=your-s3-bucket-name
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-east-1

# Email Services
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASS=your-app-password

# Push Notifications
VITE_VAPID_PUBLIC_KEY=your-vapid-public-key
VAPID_PRIVATE_KEY=your-vapid-private-key

# Development/Production Environment
NODE_ENV=development
VITE_APP_ENV=development
VITE_API_BASE_URL=http://localhost:3000/api

# Database Configuration (Production)
DATABASE_URL=postgresql://user:password@host:port/database
REDIS_URL=redis://localhost:6379

# CDN & Asset Delivery
VITE_CDN_URL=https://your-cdn.com
ASSETS_BASE_URL=https://your-assets.com

# Analytics & Tracking
VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
VITE_MIXPANEL_TOKEN=your-mixpanel-token

# Feature Flags
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_BLOCKCHAIN=false
VITE_ENABLE_ADVANCED_ANALYTICS=true
VITE_ENABLE_OFFLINE_MODE=true

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging & Debugging
LOG_LEVEL=info
VITE_DEBUG_MODE=false
ENABLE_QUERY_LOGGING=false