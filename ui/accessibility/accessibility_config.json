{
    "color_contrast_ratio": 4.5,
    "font_size": {
        "default": "16px",
        "large": "20px",
        "extra_large": "24px"
    },
    "keyboard_navigation": true,
    "screen_reader_support": true,
    "text_resize_support": true,
    "color_themes": {
        "default": {
            "background": "#FFFFFF",
            "text": "#000000",
            "primary": "#007BFF",
            "secondary": "#6C757D",
            "accent": "#28A745"
        },
        "high_contrast": {
            "background": "#000000",
            "text": "#FFFFFF",
            "primary": "#FFFF00",
            "secondary": "#00FF00",
            "accent": "#00FFFF"
        },
        "dark_mode": {
            "background": "#121212",
            "text": "#E0E0E0",
            "primary": "#BB86FC",
            "secondary": "#03DAC6",
            "accent": "#CF6679"
        }
    },
    "keyboard_shortcuts": {
        "toggle_high_contrast": "Alt+C",
        "increase_font_size": "Ctrl+Plus",
        "decrease_font_size": "Ctrl+Minus",
        "reset_font_size": "Ctrl+0",
        "skip_to_main_content": "Alt+M"
    }
}